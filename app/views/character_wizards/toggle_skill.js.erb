/*Rubymine can't format this crap :(*/
<% if @skill_toggler.not_enough_free_skill_points.present? %>
    jQuery('input#<%= params[:skill_id] -%>').removeAttr('checked');
    $.colorbox({html:'<%= escape_javascript(render(:partial => 'not_enough_points')) %>'});
<% else %>
    <% @skill_toggler.commands.each do |command| %>
         jQuery('#<%= command.keys.first %>').text('<%= command.values.first %>');
        <% if params[:value]=="true"  %>
             jQuery('#<%= command.keys.first %>').effect('highlight', {color:'green'}, 1500);
        <% else %>
             jQuery('#<%= command.keys.first %>').effect('highlight', {color:'red'}, 1500);
        <% end %>
    <% end %>
    <% @skill_toggler.skill_commands.each do |_command |  %>
    <% if _command.values.first == "enable"  %>
    jQuery('input#<%= _command.keys.first %>').removeAttr('disabled');
    <% else %>
    jQuery('input#<%= _command.keys.first %>').attr('disabled', 'true');
    <% end %>
    <% if _command.values.first == "enable"  %>
    jQuery('input#<%= _command.keys.first %>').parent().effect('highlight', {color:'green'}, 1500);
    <% else %>
    jQuery('input#<%= _command.keys.first %>').parent().effect('highlight', {color:'green'}, 1500);
    <% end %>
    <% end %>
            jQuery('#free_skill_amount').text('Pozostało wolnych umiejętności: <%= @skill_toggler.free_skill_amount %>');
        <% if params[:value]=="true"  %>
                jQuery('#free_skill_amount').effect('highlight', {color:'red'}, 1500);
        <% else %>
             jQuery('#free_skill_amount').effect('highlight', {color:'green'}, 1500);
        <% end %>
<% end %>
