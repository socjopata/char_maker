<div class="itermediate_div fltl"></div>

<TABLE WIDTH=30% CELLPADDING=4 CELLSPACING=0>
  <COL WIDTH=100*>
  <COL WIDTH=18*>
  <COL WIDTH=121*>
  <COL WIDTH=17*>
  <TR>
    <TD COLSPAN=4 WIDTH=100% VALIGN=TOP class="black alantar_td">
      <P class="white_normal">Umiejętności
        Ogólne</P>
    </TD>
  </TR>
  <% @basic_skills.in_groups_of(2, false).each do |skills| %>
      <TR VALIGN=TOP>
        <% skills.each do |skill| %>
            <TD WIDTH=39% class="alantar_td">
              <P ALIGN=CENTER class="fs_12 italic normal skill_qtip"><%= skill.name %></P>
              <p class="skill_explanation_qtip " style="display:none;">
                  <%= nice_and_shiny_description(skill)   %>
                </p>
            </TD>
            <TD WIDTH=7% class="alantar_td">
              <P ALIGN=CENTER><%= check_box_tag skill.id, 1,  @character.skills.include?(skill), :disabled => ( session[:default_skills_ids].include?(skill.id) || @cannot_select_skills.include?(skill) ) %><BR>
              </P>
            </TD>
        <% end %>
      </TR>
  <% end %>
</TABLE>


<script type="text/javascript">
  jQuery(document).ready(function() {

    $('.skill_qtip').each(function() {
        $(this).qtip(
        {
            content: $(this).next().html(),
            style: {
                 'font-size': 16,
                  classes: 'ui-tooltip-blue ui-tooltip-shadow'
            },
            position: {
                adjust: {
                x: -200
                },
                corner: {
                    target: "rightMiddle",
                    tooltip: "leftMiddle"
                }
            },
            show: {
                delay: 0
            },
            hide: {
                delay: 50,
                fixed: true
            }
        });
    });
 });
</script>

